 <Field name="Txt" type="java.lang.String"><![CDATA[UPDATE DDO_ISP_FACT t1
SET t1.MULTIPLIED_SCORE = 
(SELECT MUL*t1.score FROM 
(SELECT  ISP_TASK_ID,OUTCOME_MEASURE_ID,DDO_ISP_FACT.OM_LABEL_ID,SERVICE_PROVIDER,TIME_ID,1 / COUNT(OUTCOME_MEASURE_TASK_DOMAIN_ID) mul FROM DDO_ISP_FACT
group by ISP_TASK_ID,OUTCOME_MEASURE_ID,OM_LABEL_ID,DDO_ISP_FACT.SERVICE_PROVIDER,TIME_ID
) t2
where t1.ISP_TASK_ID = t2.ISP_TASK_ID and t1.OUTCOME_MEASURE_ID = t2.OUTCOME_MEASURE_ID 
and t1.SERVICE_PROVIDER = t2.SERVICE_PROVIDER and t1.time_id = t2.time_id and t1.om_label_id = t2.om_label_id)

