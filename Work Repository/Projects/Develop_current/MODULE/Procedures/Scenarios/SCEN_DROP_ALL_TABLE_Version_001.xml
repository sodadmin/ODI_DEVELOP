	<Field name="DefTxt" type="java.lang.String"><![CDATA[
from collections import deque
import string
import java.sql as sql
import java.lang as lang
import re

sourceConnection = odiRef.getJDBCConnection("SRC")
sqlstring = sourceConnection.createStatement()
keep_tables = ['DIM_DDO_ISP_WEIGHT']

query = '''WITH TEMP AS(select TABLE_NAME,R_CONSTRAINT_NAME  from SYS.ALL_CONSTRAINTS 
          where owner = '<?=odiRef.getSchemaNameDefaultPSchema("ODI_TARGET", "D") ?>' 
          AND CONSTRAINT_TYPE ='R') SELECT  ut.table_name, 
          case when C.TABLE_NAME is not null then c.table_name else 'null'
          end parent_tab FROM SYS.ALL_CONSTRAINTS C join TEMP on (TEMP.R_CONSTRAINT_NAME = C.CONSTRAINT_NAME) 
          right join user_tables ut on (ut.table_name = TEMP.TABLE_NAME)'''

result=sqlstring.executeQuery(query)

def make_dependency_dict(dependency_list):

    d = {}
    for entry in dependency_list:
        child, parent = entry[0], entry[1]
        #dependcy_parent_as_child =  d.get(parent, [])
        dependcy_list_child_as_child = d.get(child, [])
        if parent != 'null':
            dependcy_list_child_as_child.append(parent)
        d[child] = dependcy_list_child_as_child

    return d

def topsort(graph):

    in_degree = {}#{u: 0 for u in graph}
    for u in graph:
        in_degree[u] = 0
    # determine in-degree
    for u in graph:  # of each node
        for v in graph[u]:
            in_degree[v] += 1

    Q = deque()  # collect nodes with zero in-degree
    for u in in_degree:
        if in_degree[u] == 0:
            Q.appendleft(u)

    L = []  # list for order of nodes

    while Q:
        u = Q.pop()  # choose node of zero in-degree
        L.append(u)  # and 'remove' it from graph
        for v in graph[u]:
            in_degree[v] -= 1
            if in_degree[v] == 0:
                Q.appendleft(v)

    if len(L) == len(graph):
        return L
    else:  # if there is a cycle,
        return []  # then return an empty list


rs=[]
while result.next():
  r = [result.getString("table_name"),result.getString("parent_tab")]
  rs.append(r)

dependency_dict = make_dependency_dict(rs)
order = topsort(dependency_dict)


for table_name in order:
  if '$' not in table_name and table_name not in keep_tables:
    sqlstring.executeUpdate("drop table "+table_name + " purge")
sourceConnection.close()]]></Field>
