	<Field name="DefTxt" type="java.lang.String"><![CDATA[import logging
import string
import java.sql as sql
import java.lang as lang
import re
import smtplib
from email.MIMEMultipart import MIMEMultipart
from email.MIMEText import MIMEText

def reindent(s, numSpaces):
    s = string.split(s, '\n')
    s = [(numSpaces * ' ') + string.lstrip(line) for line in s]
    s = string.join(s, '\n')
    return s

logger = logging.getLogger('#ETL.LOGGER')

sourceConnection = odiRef.getJDBCConnection("SRC")
sqlstring = sourceConnection.createStatement()
msg_body = ""

current_load_plan_query = "select i_lp_inst from snp_lpi_exc_log where sess_no=<?=odiRef.getSession("SESS_NO") ?>"
current_load_plan_no = sqlstring.executeQuery(current_load_plan_query)
lp = ""

while current_load_plan_no.next():
  lp = current_load_plan_no.getString("i_lp_inst")
  break
current_load_plan_no.close()
lp = str(lp)

error_message_query = "select error_message from SNP_LPI_STEP_LOG where i_lp_inst ="+lp +" order by start_date desc fetch first 1 row only"
result_error_message_query = sqlstring.executeQuery(error_message_query)
while result_error_message_query.next():
  msg_body = str(result_error_message_query.getString("error_message"))
  
result_error_message_query.close()

msg_body = msg_body+"\n\n"+"CONTEXT: "+"<?=odiRef.getContext("CTX_NAME") ?>"



s = smtplib.SMTP()
s.connect(#ETLFORSELECTEDPROVIDER.MAIL_SERVER, 25)

fromaddr = #ETLFORSELECTEDPROVIDER.MAIL_SENDER
toaddr = #ETLFORSELECTEDPROVIDER.MAIL_RECEIPIENT_LIST.split(",")
msg = MIMEMultipart()
msg['From'] = fromaddr
msg['To'] = ', '.join(toaddr)
msg['Subject'] = "ETL Execution Failure Notification"

msg.attach(MIMEText(msg_body, 'plain'))
text = msg.as_string()

s.sendmail("arafat@therapservices.net", toaddr, text)

sourceConnection.close()]]></Field>
